{
  "name": "risky-pipes-plugin",
  "version": "1.0.0",
  "type": "module",
  "description": "Sample plugin for Qatium using typescript",
  "scripts": {
    "clean": "rimraf dist src/public/plugin.js",
    "dev": "tsx build.config.ts --dev",
    "build": "tsc --project src/panel/tsconfig.json && tsc --project src/plugin/tsconfig.json && tsx build.config.ts",
    "test": "jest --verbose -i",
    "test:watch": "pnpm run test --watchAll -i --no-cache"
  },
  "devDependencies": {
    "@qatium/sdk-testing-library": "^1.0.9",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/geojson": "^7946.0.14",
    "@types/jest": "^29.5.12",
    "@types/js-quantities": "^1.6.6",
    "@types/react": "18.2.48",
    "@types/react-dom": "^18.2.19",
    "@vitejs/plugin-react": "^4.3.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^3.3.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.7",
    "typescript": "~5.5.2",
    "vite": "^5.4.10",
    "vite-plugin-singlefile": "^2.0.2",
    "vite-plugin-svgr": "^4.2.0"
  },
  "dependencies": {
    "@qatium/sdk": "^0.0.58",
    "i18n": "^0.15.1",
    "i18next": "^24.2.2",
    "react": "18.3.1",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.4.0",
    "theme-ui": "^0.17.2"
  }
}
